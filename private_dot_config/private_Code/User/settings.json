{
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[dotenv]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "[properties]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[svg]": {
        "editor.defaultFormatter": "jock.svg"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.language": "en-GB,en",
    "cSpell.userWords": [
        "atexit",
        "clsx",
        "elgato",
        "jsonify",
        "kubernetes",
        "monzo",
        "oauth",
        "preinstall",
        "rbac",
        "sourcemap",
        "uuid",
        "Vendittelli"
    ],
    "conventionalCommits.gitmoji": false,
    "dev.containers.defaultExtensions": [
        "eamodio.gitlens",
        "mutantdino.resourcemonitor",
        "gruntfuggly.todo-tree",
        "ms-vsliveshare.vsliveshare"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.containers.label": "RepositoryNameAndTag",
    "docker.images.groupBy": "RepositoryName",
    "editor.bracketPairColorization.enabled": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "Fira code",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": "active",
    "editor.inlineSuggest.enabled": true,
    "editor.linkedEditing": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80,
        120
    ],
    "explorer.confirmDelete": false,
    "files.associations": {
        "*.py": "python"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1500,
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.fetchOnPull": true,
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": false,
        "scminput": false
    },
    "githubPullRequests.pullBranch": "never",
    "gitlens.codeLens.enabled": false,
    "gitlens.defaultDateFormat": "YYYY-MM-DDTHH:mm:ssZ",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "gitlens.views.branches.branches.layout": "list",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "material-icon-theme.activeIconPack": "react_redux",
    "material-icon-theme.files.associations": {
        "*.slice.ts": "redux-reducer"
    },
    "material-icon-theme.folders.associations": {
        "cypress": "test",
        "redux": "redux-store",
        "slices": "redux-reducer"
    },
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "open-in-browser.default": "Google Chrome",
    "python.formatting.provider": "black",
    "python.linting.lintOnSave": true,
    "redhat.telemetry.enabled": false,
    "remote.SSH.remotePlatform": {
        "pi-home": "linux",
        "clock": "linux"
    },
    "security.promptForLocalFileProtocolHandling": false,
    "task.allowAutomaticTasks": "on",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.external.windowsExec": "C:\\Users\\samve\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "terminal.integrated.shellIntegration.enabled": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.commandCenter": true,
    "workbench.colorTheme": "Dracula",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.iconTheme": "material-icon-theme",
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "codeQL.telemetry.enableTelemetry": false
}
